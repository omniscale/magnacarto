Map { background-color: black; }

#test::label{
    text-name: [name];
    text-size: "[size]";
    text-fill: "[color]";
    text-halo-radius: "min([size], [halo]) / 2";
    text-halo-fill: #fff;
    text-orientation: "[angle]";
    text-allow-overlap: "[overlap]";


    text-face-name: "Noto Sans Regular";

    text-placement: point;
}
