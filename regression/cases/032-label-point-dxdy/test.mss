
#test::label{
    text-name: [name];
    text-size: 14;
    text-halo-radius: 2;
    text-halo-fill: #fff;

    text-face-name: "Noto Sans Regular";

    text-placement: point;

    [id=1] { text-dx: 5; }
    [id=2] { text-dy: 5; }
    [id=3] { text-dx: 5; text-dy: 5; }
    [id=4] { text-dx: -5; text-dy: -5; }
}
