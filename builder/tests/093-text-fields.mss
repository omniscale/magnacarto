#labels_field {
    text-name: [name];
    text-size: 8;
}

#labels_fieldstring {
    text-name: "[name]";
    text-size: 8;
}

#labels_string {
    text-name: "name";
    text-size: 8;
}

#labels_stringexpr {
    text-name: "foo" + "bar";
    text-size: 8;
}

#labels_mixedconcat {
    text-name: "[name]" + " (" + [elev] + ")";
    text-size: 8;
}
