#lines {
  [cap='round']  { line-cap: round; }
  [cap='butt']   { line-cap: butt; }
  [cap='square'] { line-cap: square; }
}

#lines {
  [join='miter'] { line-join: miter; }
  [join='round'] { line-join: round; }
  [join='bevel'] { line-join: bevel; }
}

#lines [type='capjoin'] {
  line-width: 1;
}
